    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --project chromium --headed",
    "test:mobile": "playwright test --project 'Mobile Chrome'",
    "test:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "supabase": "node scripts/supabase-manager.js",
    "db:test": "node scripts/supabase-manager.js test",
    "db:inspect": "node scripts/supabase-manager.js inspect",
    "db:migrate": "node scripts/supabase-manager.js migrate"
