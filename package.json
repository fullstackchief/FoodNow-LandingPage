{
  "name": "foodnow-landingpage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo -p ${PORT:-3000}",
    "dev:3002": "PORT=3002 next dev --turbo -p 3002",
    "dev:clean": "./scripts/clean-start.sh",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
